#include <iostream>

#include "Advance.h"
#include "Template.h"

using namespace std;

int main() {

	Advance a(1, 2);
	Advance newA;
	Advance add;
	Advance copy = a;

	a.printXY();
	newA.printXY();
	newA = a;
	newA.printXY();
	add = a + newA;
	add.printXY();

	return 0;
}
