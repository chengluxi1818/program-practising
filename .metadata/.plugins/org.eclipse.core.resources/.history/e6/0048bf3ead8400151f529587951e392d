
public class Main {
	public static int[] rotate(int[] theArray, int pivot) {
		for(int i = pivot; i < theArray.length; i++) {
			int j = i;
			int count = pivot;
			while(pivot != 0) {
				int temp = theArray[j-1];
				theArray[j-1] = theArray[j];
				theArray[j] = temp;
			}
		}
		
		return theArray;
	}
}
